pipeline{
  agent any

  stages{
    stage('Build'){
      steps{
          sh 'cd /var/lib/jenkins/workspace/leafer'
          sh 'mvn clean package'
      }
    }

    stage('Deploy'){
      steps{
        sh 'ansible-playbook /var/lib/jenkins/workspace/leafer/scripts/deploy.yml'
      }
    }
  }
}